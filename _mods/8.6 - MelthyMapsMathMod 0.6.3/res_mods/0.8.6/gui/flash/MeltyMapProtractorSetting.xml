<mmps>
  <config>
    <enableEffArmor>true</enableEffArmor>    <!-- Отображать на экране калькулятор бронирования. * только для танков, которые есть в разделе "vehiclesData". -->
    <enableRulerWhenTD>true</enableRulerWhenTD>    <!-- Углы горизонтальной наводки для ПТ. -->
    <enableRulerWhenSPG>true</enableRulerWhenSPG>    <!-- Углы горизонтальной наводки для Арты. -->
    <minimapEx>true</minimapEx>    <!-- Включить доп информацию на мини карте. -->
    <damagePanelEx>true</damagePanelEx>  <!-- Дамаг панель. (Отображать время ремонта модуля). -->
    <bigMapKey>20</bigMapKey>    <!-- Зумм миникарты, по умолчанию 20(CapsLock), или 17(Ctrl). Коды других кнопок http://www.cambiaresearch.com/articles/15/javascript-char-codes-key-codes -->
    <bigMapSize>700</bigMapSize>   <!-- Размер миникарты в пикселях. -->
    <ordinateWhenTank>0.65</ordinateWhenTank>    <!-- ** Вертикальное размещение эффективное для расчёта брони противника(Лучше не трогать) . -->
    <ordinateWhenTD>0.65</ordinateWhenTD>    <!-- ** Вертикальное размещение оружиеу ПТ(Лучше не трогать) (от 0 до 1). -->
    <ordinateWhenSPG>0</ordinateWhenSPG>    <!-- ** Вертикальное размещение оружиеу Арт(Лучше не трогать) -->
    <timeOrange>300</timeOrange> <!-- Время до конца боя, когда оно начинает мигать ораньжевым. -->
    <offsetX>-370</offsetX> <!-- Размещение часов в px. По умолчанию (вверхний правый угол) -370-->
    <offsetY>15</offsetY> <!-- Размещение часов в px. По умолчанию (вверхний правый угол) 15-->
    <m2pix>8.2</m2pix>    <!-- Соотношение пикселей к расстоянию, по умолчанию 1m = 8.2px. -->
    <xSpacing>0</xSpacing> <!-- ** Настройки углов оружия, по умолчанию 0 адаптивный. Для ручного рекомендуется от 16 до 32.(не трограть) -->
    <enableClock>true</enableClock> <!-- Отображать в бою реальное время. -->
    <circle>445</circle> <!-- ** Знаменитые 445 метров(не трогать) -->
  </config>
