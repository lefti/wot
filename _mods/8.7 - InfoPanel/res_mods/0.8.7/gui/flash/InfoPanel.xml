<?xml version="1.0" encoding="utf-8"?>
<ip>

	<!-- Author: Shtys -->
	<!-- Modified and translated by furs -->

	<!-- Available options:
	{{n}}					- переход на новую строку
	{{t}}					- фиксированный отступ (равнозначен TAB)
	{{target}}				- название танка выбранной цели
	{{target-gun}}			- названия орудия танка цели
	{{target-reload-100}}	- примерное время перезарядки орудия танка цели с округлением до сотых (с досылателем и 110% экипажем)
	{{target-reload-10}}	- примерное время перезарядки орудия танка цели с округлением до десятых (с досылателем и 110% экипажем)
	{{target-reload}}		- примерное время перезарядки орудия танка цели с округлением до целых (с досылателем и 110% экипажем)
	{{vision}}				- радиус обзора танка цели (для 100% экипажа)
	{{hull-front}}			- лобовое бронирование корпуса танка цели
	{{hull-side}}			- бортовое бронирование корпуса танка цели
	{{hull-back}}			- бронирование кормы корпуса танка цели
	{{turret-front}}		- лобовое бронирование башни танка цели
	{{turret-side}}			- бортовое бронирование башни танка цели
	{{turret-back}}			- бронирование кормы башни танка цели
	{{damage-1}}			- урон первого типа снаряда танка цели
	{{damage-2}}			- урон второго типа снаряда танка цели
	{{damage-3}}			- урон третьего типа снаряда танка цели
	{{power-1}}				- бронепробиваемость первого типа снаряда танка цели
	{{power-2}}				- бронепробиваемость второго типа снаряда танка цели
	{{power-3}}				- бронепробиваемость третьего типа снаряда танка цели
	{{type-1}}				- первый тип снаряда танка цели
	{{type-2}}				- второй тип снаряда танка цели
	{{type-3}}				- третий тип снаряда танка цели -->

	<!-- Глобальные настройки панели информации о цели -->
	<infoPanel>
		<!-- Settings for different modes -->
		<arcade visible="true">
			<location x="-250" y="250"/>
			<delay>5</delay> <!-- Time after panel disappears. 0 - always on -->
		</arcade>
		<sniper visible="true">
			<location x="-250" y="250"/>
			<delay>5</delay> 
		</sniper>
		<strategic visible="true">
			<location x="-250" y="250"/>
			<delay>15</delay>
		</strategic>
		<postmortem visible="true">
			<location x="-250" y="250"/>
			<delay>10</delay>
		</postmortem>
	</infoPanel>
	
	<background alpha="0">
		<scale x="85" y="118"/>
	</background>
	
	<targetName visible="true">
		<location x="0" y="-110"/>
		<text font="Cuprum" size="12" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="300" height="35"/>
		<!-- align - left/right/center. leading - line spacing. -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> 
		<format>{{target}}</format>
		<!-- Its formed from a macro and (or) any characters. 
			 List of macros listed at the beginning of the config-->
	</targetName>
	
	<gunName visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-80" y="110"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="300" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{target-gun}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</gunName>
	
	<!-- Настройка примерного времени перезарядки орудия танка цели (с досылателем и 110% экипажем) -->
	<gunReload visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="120" y="-110"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="12" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{target-reload-10}} s</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</gunReload>
	
	<!-- Настройка радиуса обзора танка цели -->
	<visionRadius visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="0" y="-95"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="12" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>View range:{{t}}{{vision}} m</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</visionRadius>
	
	<!-- turret front -->
	<turretArmorF visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="0" y="-80"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="13" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>Turret:{{t}}{{turret-front}}{{t}}{{turret-side}}{{t}}{{turret-back}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</turretArmorF>
	
	<!-- Настройка бортового бронирования башни -->
	<turretArmorS visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-55" y="0"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFF00" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{turret-side}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</turretArmorS>
	
	<!-- Настройка бронирования кормы башни -->
	<turretArmorB visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-30" y="0"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0x00FF00" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{turret-back}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</turretArmorB>
	
	<!-- Настройка лобового бронирования корпуса -->
	<hullArmorF visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="0" y="-65"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="12" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>Hull:{{t}}{{t}}{{hull-front}}{{t}}{{hull-side}}{{t}}{{hull-back}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</hullArmorF>
	
	<!-- Настройка бортового бронирования корпуса -->
	<hullArmorS visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-105" y="85"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFF00" alpha="100" align="right" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{hull-side}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</hullArmorS>
	
	<!-- Настройка бронирования кормы корпуса -->
	<hullArmorB visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-80" y="85"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0x00FF00" alpha="100" align="right" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{hull-back}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</hullArmorB>
	
	
	<shellType1 visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="0" y="-20"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="12" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>Ammo:  {{t}}{{type-1}}{{t}}{{type-2}}{{t}}{{type-3}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</shellType1>
	
	<!-- Настройка типа второго снаряда -->
	<shellType2 visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="150" y="20"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{type-2}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</shellType2>
	
	<!-- Настройка типа третьего снаряда -->
	<shellType3 visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-110" y="20"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{type-3}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</shellType3>
			
